{
  "name": "@socialgouv/datafiller-data",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0",
  "main": "index.js",
  "files": [
    "data"
  ],
  "homepage": "https://github.com/SocialGouv/datafiller-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/datafiller-data.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "start": "NODE_OPTIONS=--insecure-http-parser ts-node src/index.ts",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.5",
    "node-fetch": "^2.6.0",
    "p-pipe": "^3.0.0",
    "remark": "^11.0.2",
    "remark-html": "^11.0.1"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-recommended": "^0.19.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "esm": "^3.2.25",
    "husky": "^4.2.5",
    "jest": "^25.2.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint . --ext .ts",
      "pre-push": "run-p lint test"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
